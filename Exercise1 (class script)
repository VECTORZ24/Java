public class MATH {

    // Method to compute the mean
    public static double computeMean(double[] numbers) {
        double sum = 0;
        for (double num : numbers) {
            sum += num;
        }
        return sum / numbers.length;
    }

    // Method to compute the standard deviation
    public static double computeStandardDeviation(double[] numbers, double mean) {
        double varianceSum = 0;
        for (double num : numbers) {
            varianceSum += Math.pow(num - mean, 2);
        }
        return Math.sqrt(varianceSum / numbers.length);  // population std deviation
    }
}
